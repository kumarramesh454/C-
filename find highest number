#include <stdio.h>

int main() {
      int num1,num2,num3;
     printf("Enter number 1: ");
     scanf("%d",&num1);
     printf("Enter number 2: ");
     scanf("%d",&num2);
     printf("Enter number 3: ");
     scanf("%d",&num3);
     
     if(num1 > num2)
     {
         printf("First number is greater!",num1,num2,num1>num2);
     }
     else if(num2 > num3)
     {
      printf("Second number is greater!",num2,num3,num2>num3);
     }
     else
     {
         printf("Third number is greater!");
     }
    
    return 0;
}
